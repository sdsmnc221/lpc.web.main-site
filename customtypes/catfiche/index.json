{
  "format": "custom",
  "id": "catfiche",
  "label": "CatFiche",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "createddate": {
        "type": "Date",
        "config": {
          "label": "publicationDate",
          "placeholder": ""
        }
      },
      "catname": {
        "type": "Text",
        "config": {
          "label": "catName",
          "placeholder": ""
        }
      },
      "catphoto": {
        "type": "Image",
        "config": {
          "label": "catPhoto",
          "constraint": {},
          "thumbnails": []
        }
      },
      "catphoto2": {
        "type": "Image",
        "config": {
          "label": "catPhoto2",
          "constraint": {},
          "thumbnails": []
        }
      },
      "catsexe": {
        "type": "Select",
        "config": {
          "label": "catSexe",
          "placeholder": "",
          "options": ["♂️", "♀️"]
        }
      },
      "catagenumber": {
        "type": "Number",
        "config": {
          "label": "catAgeNumber",
          "placeholder": ""
        }
      },
      "catagetype": {
        "type": "Select",
        "config": {
          "label": "catAgeType",
          "placeholder": "",
          "options": ["Mois", "An(s)"]
        }
      },
      "catbirth": {
        "type": "Date",
        "config": {
          "label": "catBirth",
          "placeholder": ""
        }
      },
      "catbirthyear": {
        "type": "Text",
        "config": {
          "label": "catBirthYear",
          "placeholder": ""
        }
      },
      "catdescription": {
        "type": "StructuredText",
        "config": {
          "label": "catDescription",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,strong,em,hyperlink,list-item,o-list-item,rtl"
        }
      },
      "catidentification": {
        "type": "Select",
        "config": {
          "label": "catIdentification",
          "placeholder": "",
          "options": [
            "Identifié.e par tatouage",
            "Identifié.e par puce",
            "Sera identifié.e avant l'adoption."
          ],
          "default_value": "Identifié.e par tatouage"
        }
      },
      "catvaccination": {
        "type": "Boolean",
        "config": {
          "label": "catVaccination",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": true
        }
      },
      "catsterilization": {
        "type": "Boolean",
        "config": {
          "label": "catSterilization",
          "placeholder_false": "false",
          "placeholder_true": "true",
          "default_value": false
        }
      },
      "zipcode": {
        "type": "Number",
        "config": {
          "label": "zipCode",
          "placeholder": "91210"
        }
      },
      "relatedcat": {
        "type": "Link",
        "config": {
          "label": "relatedCat",
          "select": "document",
          "customtypes": ["catfiche"]
        }
      },
      "adoptionstatus": {
        "type": "Select",
        "config": {
          "label": "adoptionStatus",
          "placeholder": "",
          "options": [
            "Adoptable - Chat adulte",
            "Adoptable - Chat craintif",
            "Adoptable - Chaton",
            "Adopté.e",
            "Adoptable - Chat ado",
            "Réservé.e"
          ],
          "default_value": "Adoptable - Chat adulte"
        }
      },
      "isfosterpossible": {
        "type": "Boolean",
        "config": {
          "label": "isFosterPossible",
          "default_value": false,
          "placeholder_true": "true",
          "placeholder_false": "false"
        }
      }
    }
  }
}
